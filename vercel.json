{
    "version": 2,
    "functions": {
      "./index.js": {
        "memory": 128,
        "maxDuration": 10
      },
      "api/scheduled.js": {
        "schedule": "0 * * * *"  
      }
    },
    "routes": [
      {
        "src": "./",
        "dest": "/index.js"
      },
      {
        "src": "./scheduled",
        "dest": "./scheduled.js"
      }
    ]
  }
  
